{
  "name": "nosqwal-couchbase",
  "version": "0.2.1",
  "description": "Nosqwal adapter for couchbase",
  "license": "MIT",
  "repository": "mastilver/nosqwal-couchbase",
  "author": {
    "name": "Thomas Sileghem",
    "email": "th.sileghem@gmail.com",
    "url": "mastilver.com"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "pretest": "rm -rf test && cp -r node_modules/nosqwal-test/test test",
    "test": "xo && nyc ava",
    "setup-couchbase": "node scripts/setup-couchbase.js"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "couchbase",
    "nosqwal",
    "odm"
  ],
  "dependencies": {
    "couchbase": "^2.2.4",
    "cuid": "^1.3.8",
    "p-wait-for": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "coveralls": "^2.11.12",
    "execa": "^0.5.0",
    "nosqwal-test": "^0.4.3",
    "nyc": "^10.0.0",
    "xo": "^0.17.1"
  },
  "xo": {
    "esnext": true,
    "space": 4,
    "ignores": [
      "**/coverage/**"
    ],
    "rules": {
      "ava/no-ignored-test-files": "off",
      "eqeqeq": "off",
      "no-eq-null": "off"
    }
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "ava": {
    "require": [
      "./setup-test.js"
    ]
  }
}
